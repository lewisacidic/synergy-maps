@article{Cokol:2011ck,
author = {Cokol, Murat and Chua, Hon Nian and Tasan, Murat and Mutlu, Beste and Weinstein, Zohar B and Suzuki, Yo and Nergiz, Mehmet E and Costanzo, Michael and Baryshnikova, Anastasia and Giaever, Guri and Nislow, Corey and Myers, Chad L and Andrews, Brenda J and Boone, Charles and Roth, Frederick P},
title = {{Systematic exploration of synergistic drug pairs}},
journal = {Molecular Systems Biology},
year = {2011},
volume = {7},
pages = {544},
month = nov,
affiliation = {Department of Biological Chemistry and Molecular Pharmacology, Harvard Medical School, Boston, MA, USA. cokol@sabanciuniv.edu},
keywords = {recommended by Andreas},
doi = {10.1038/msb.2011.71},
pmid = {22068327},
pmcid = {PMC3261710},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-01-28T12:04:59GMT},
date-modified = {2014-11-25T16:29:41GMT},
abstract = {Drug synergy allows a therapeutic effect to be achieved with lower doses of component drugs. Drug synergy can result when drugs target the products of genes that act in parallel pathways ('specific synergy'). Such cases of drug synergy should tend to correspond to synergistic genetic interaction between the corresponding target genes. Alternatively, 'promiscuous synergy' can arise when one drug non-specifically increases the effects of many other drugs, for example, by increased bioavailability. To assess the relative abundance of these drug synergy types, we examined 200 pairs of antifungal drugs in S. cerevisiae. We found 38 antifungal synergies, 37 of which were novel. While 14 cases of drug synergy corresponded to genetic interaction, 92% of the synergies we discovered involved only six frequently synergistic drugs. Although promiscuity of four drugs can be explained under the bioavailability model, the promiscuity of Tacrolimus and Pentamidine was completely unexpected. While many drug synergies correspond to genetic interactions, the majority of drug synergies appear to result from non-specific promiscuous synergy.},
url = {http://msb.embopress.org/cgi/doi/10.1038/msb.2011.71},
local-url = {file://localhost/Users/RichLewis/Dropbox/Library.papers3/Files/D8/D8852092-3133-4964-A4B0-0AACA713DEEC.pdf},
file = {{D8852092-3133-4964-A4B0-0AACA713DEEC.pdf:/Users/RichLewis/Dropbox/Library.papers3/Files/D8/D8852092-3133-4964-A4B0-0AACA713DEEC.pdf:application/pdf;D8852092-3133-4964-A4B0-0AACA713DEEC.pdf:/Users/RichLewis/Dropbox/Library.papers3/Files/D8/D8852092-3133-4964-A4B0-0AACA713DEEC.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1038/msb.2011.71}}
}

@article{Severyn:2011gu,
author = {Severyn, Bryan and Liehr, Robert A and Wolicki, Alex and Nguyen, Kevin H and Hudak, Edward M and Ferrer, Marc and Caldwell, Jeremy S and Hermes, Jeffrey D and Li, Jing and Tudor, Matthew},
title = {{Parsimonious discovery of synergistic drug combinations.}},
journal = {ACS Chemical Biology},
year = {2011},
volume = {6},
number = {12},
pages = {1391--1398},
month = dec,
affiliation = {Merck Research Laboratories, North Wales, Pennsylvania 19454, USA.},
doi = {10.1021/cb2003225},
pmid = {21974780},
language = {English},
read = {Yes},
rating = {0},
date-added = {2014-11-24T18:52:09GMT},
date-modified = {2014-11-25T17:21:01GMT},
abstract = {Combination therapies that enhance efficacy or permit reduced dosages to be administered have seen great success in a variety of therapeutic applications. More fundamentally, the discovery of epistatic pathway interactions not only informs pharmacologic intervention but can be used to better understand the underlying biological system. There is, however, no systematic and efficient method to identify interacting activities as candidates for combination therapy and, in particular, to identify those with synergistic activities. We devised a pooled, self-deconvoluting screening paradigm for the efficient comprehensive interrogation of all pairs of compounds in 1000-compound libraries. We demonstrate the power of the method to recover established synergistic interactions between compounds. We then applied this approach to a cell-based screen for anti-inflammatory activities using an assay for lipopolysaccharide/interferon-induced acute phase response of a monocytic cell line. The described method, which is >20 times as efficient as a na{\"\i}ve approach, was used to test all pairs of 1027 bioactive compounds for interleukin-6 suppression, yielding 11 pairs of compounds that show synergy. These 11 pairs all represent the same two activities: $\beta$-adrenergic receptor agonists and phosphodiesterase-4 inhibitors. These activities both act through cyclic AMP elevation and are known to be anti-inflammatory alone and to synergize in combination. Thus we show proof of concept for a robust, efficient technique for the identification of synergistic combinations. Such a tool can enable qualitatively new scales of pharmacological research and chemical genetics.},
url = {http://pubs.acs.org/doi/abs/10.1021/cb2003225},
local-url = {file://localhost/Users/RichLewis/Dropbox/Library.papers3/Files/9B/9B34BE02-BFD6-4646-A9E6-7D36042863F2.pdf},
file = {{9B34BE02-BFD6-4646-A9E6-7D36042863F2.pdf:/Users/RichLewis/Dropbox/Library.papers3/Files/9B/9B34BE02-BFD6-4646-A9E6-7D36042863F2.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1021/cb2003225}}
}

