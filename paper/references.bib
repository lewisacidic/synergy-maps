title = {{Modelling of Compound Combination Effects and Applications to Efficacy and Toxicity: State-of-the-Art, Challenges and Perspectives}},
journal = {Drug Discovery Today},
rating = {0},
date-added = {2015-03-26T12:17:41GMT},
date-modified = {2015-03-26T12:23:47GMT},
abstract = {The development of treatments involving combinations of drugs is a logical approach towards combating complex or multifactorial disorders. However, the large number of compound combinations that can be generated, from even small compound collections, means that exhaustive experimental testing infeasible. The ability to predict the behaviour of compound combinations in biological systems, whittling down the number of combinations to be tested, is therefore key. Here we review the current state-of-the-art in the field of compound combination modelling, with the aim to support the development of approaches that, as we hope, will finally lead to an integration of chemical with systems-level biological information for predicting the effect of chemical mixtures. },
uri = {\url{papers3://publication/uuid/E65CB3A7-AE7B-4FA5-A935-3EC19747AF88}}
}

