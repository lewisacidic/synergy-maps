@article{Zimmermann:2007iz,
author = {Zimmermann, Grant R and Keith, Curtis T and Leh{\'a}r, Joseph},
title = {{Multi-target therapeutics: when the whole is greater than the sum of the parts}},
journal = {Drug Discovery Today},
year = {2007},
volume = {12},
number = {1-2},
pages = {34--42},
month = jan,
annote = {Intro:
Single molecule treatments not effective due to redundancy and drug-mitigating responses such as metabolism.
        
Instead, use combinations of drugs to attack system from multiple angles.
        
This approach has been employed using drugs developed to work by themselves (monotherapy).  This is a waste of resources.
        
Rationale for multi-target therapeutics:
        
Disease state has changes in expression in multiple pathways, which each require correcting.
        
This is hard to do by influencing a single target, so modulation of multiple targets might be required to achieve optimal therapeutic benefit.
e.g. Cancer MUST be modulation of multiple targets as 4-7 mutations are required.},
keywords = {recommended by Andreas},
doi = {10.1016/j.drudis.2006.11.008},
pmid = {17198971},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-11-11T23:47:41GMT},
date-modified = {2014-11-25T17:20:03GMT},
abstract = {Drugs designed to act against individual molecular targets cannot usually combat multigenic diseases such as cancer, or diseases that affect multiple tissues or cell types such as diabetes and immunoinflammatory disorders. Combination drugs that impact multiple targets simultaneously are better at controlling complex disease systems, are less prone to drug resistance and are the standard of care in many important therapeutic areas. The combination drugs currently employed are primarily of rational design, but the increased efficacy they provide justifies in vitro discovery efforts for identifying novel multi-target mechanisms. In this review, we discuss the biological rationale for combination therapeutics, review some existing combination drugs and present a systematic approach to identify interactions between molecular pathways that could be leveraged for therapeutic benefit.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1359644606004697},
